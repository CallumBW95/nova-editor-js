{
    "packages": {
        "callumbw95/nova-editor-js": {
            "1.0.0": {
                "name": "CallumBW95/nova-editor-js",
                "version": "1.0.0",
                "description": "A Laravel Nova field bringing EditorJs magic to Nova.",
                "keywords": [
                    "laravel",
                    "nova",
                    "editor",
                    "editorjs",
                    "wysiwyg"
                ],
                "license": "MIT",
                "require": {
                    "php": ">=7.1.0",
                    "ext-exif": "*",
                    "ext-json": "*",
                    "codex-team/editor.js": "*",
                    "spatie/image": "^1.7"
                },
                "autoload": {
                    "psr-4": {
                        "Advoor\\NovaEditorJs\\": "src/"
                    }
                },
                "extra": {
                    "laravel": {
                        "providers": [
                            "Advoor\\NovaEditorJs\\FieldServiceProvider"
                        ]
                    }
                },
                "config": {
                    "sort-packages": true
                },
                "minimum-stability": "dev",
                "prefer-stable": true
            }
        }
    }
}
